<table class="table table-striped">
  <thead>
    <tr>
      <th>N°</th>
      <th></th>
      <th>Jour</th>
      <th>Début</th>
      <th>Fin</th>
      <th>Field</th>
      <th>Sportizers</th>
      <th></th>
    </tr>
  </thead>
  <tbody class="match"><p>
    <% @matches.each do |match| %>
        <tr>
          <td><%= match.id %></td>
          <td></td>
          <td><%= match.jour.strftime("%d/%m/%y") %></td>
          <td><%= match.start %></td>
          <td><%= match_end(match) %></td>
          <td><%= match.field.name %></td>
          <td><%= sportizers_name(match) %></td>
          <td><%= submit_tag "Participer", disabled: true, class: "btn btn-small" %></td>
        </tr>
    <% end %>
    <tr>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td><%= link_to "Nouveau", new_match_path, class: "btn btn-small" %></td>
    </tr>
  </tbody>
</table>